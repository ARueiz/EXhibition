<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>展覽</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">

    @*@Scripts.Render("~/bundles/modernizr")*@
    <script src="https://code.jquery.com/jquery-3.6.0.js"
            integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
            crossorigin="anonymous"></script>
    @Styles.Render("~/Content/navbar.css")
    @Scripts.Render("~/Scripts/PartView/navbar.js")
    @Styles.Render("~/Content/w3.css")
    @Styles.Render("~/Content/Bootstrap/bootstrap.min.css")
    @Scripts.Render("~/Scripts/Bootstrap/bootstrap.bundle.min.js")

    <meta name="description" content="資展國際 專題 2022/04/15 集合各類活動的網站" />
    <meta name="keywords" content="展覽" />
    <meta name="keywords" content="活動" />
